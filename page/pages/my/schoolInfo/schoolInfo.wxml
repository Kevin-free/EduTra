<!--pages/my/schoolInfo/schoolInfo.wxml-->
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">学校名称</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{schoolName}}" name="schoolName"></input>
    </view>
  </view>
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">学校地址</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" type="text" disabled="{{true}}" value="{{schoolAddress}}" name="schoolAddress"></input>
    </view>
  </view>
</view>